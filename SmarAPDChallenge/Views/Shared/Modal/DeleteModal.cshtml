<!-- Delete Modal -->
<div class="modal fade" id="deleteModal" tabindex="-1" role="dialog" aria-labelledby="deleteModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="deleteModalLabel">Delete Entry</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                Are you sure about this?
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-secondary dimiss-delete" data-dismiss="modal">No, I don't</button>
                <button type="button" class="btn btn-outline-danger" id="deleteBtn" data-url="">Yes, Do it!</button>
            </div>
        </div>
    </div>
</div>